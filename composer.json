{
  "name": "provider/european-central-bank",
  "description": "European Central Bank data provider for Porter.",
  "authors": [
    {
      "name": "Bilge",
      "email": "bilge@scriptfusion.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/abbajbryant/Porter.git"
    }
  ],
  "require": {
    "ext-simplexml": "*",
    "amphp/amp": "^2.1",
    "amphp/process": "^1.1",
    "amphp/parallel": "^1.1",
    "connectors/http": "^3",
    "scriptfusion/porter": "dev-master as 5.2.1"
  },
  "require-dev": {
    "mockery/mockery": "^1.3",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "ScriptFUSION\\Porter\\Provider\\EuropeanCentralBank\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ScriptFUSIONTest\\Porter\\Provider\\EuropeanCentralBank\\": "test"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c ./test/phpunit.xml ./test"
  },
  "config": {
    "sort-packages": true
  }
}
